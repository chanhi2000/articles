{
  "name": "bookshelf",
  "description": "Bookshelf",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vuepress-vite dev src",
    "clean-dev": "vuepress-vite dev src --clean-cache",
    "build": "NODE_OPTIONS=--max-old-space-size=16432 vuepress-vite build src",
    "clean-build": "vuepress-vite build src --clean-cache",
    "mem-check": "node -e 'console.log(v8.getHeapStatistics().heap_size_limit/(1024*1024))'",
    "update-package": "npx vp-update"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-rc.14",
    "rollup-plugin-visualizer": "5.12.0",
    "vuepress-plugin-markdown-define2": "^1.0.0",
    "vue": "3.4.33",
    "vuepress": "2.0.0-rc.14",
    "vuepress-theme-hope": "2.0.0-rc.52"
  },
  "dependencies": {
    "@vue/repl": "4.1.1",
    "@vuepress/plugin-prismjs": "2.0.0-rc.39",
    "chart.js": "4.4.3",
    "cross-env": "7.0.3",
    "dashjs": "4.7.4",
    "echarts": "5.5.1",
    "flowchart.ts": "3.0.0",
    "hls.js": "1.4.12",
    "kotlin-playground": "1.30.0",
    "mermaid": "10.9.1",
    "sandpack-vue3": "3.1.11",
    "sass-loader": "14.2.1",
    "vidstack": "1.11.29"
  }
}
